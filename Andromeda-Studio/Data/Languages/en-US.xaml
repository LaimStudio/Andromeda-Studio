<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:v="clr-namespace:System;assembly=mscorlib">

    <v:String x:Key="@Project">Project</v:String>
    <v:String x:Key="@File">File</v:String>
    
    <v:String x:Key="@Create">Create</v:String>
    <v:String x:Key="@Open">Open</v:String>
    <v:String x:Key="@Edit">Edit</v:String>

    <v:String x:Key="@View">View</v:String>
    <v:String x:Key="@Help">Help</v:String>

    <v:String x:Key="@Settings">Settings</v:String>
    
    <v:String x:Key="@Exit">Exit</v:String>

</ResourceDictionary>